#include <iostream>

using namespace std;

int main()
{
    int a, b;
    cin>>a>>b;
    if((b>12)||(a>31)||(b==2 && a>28)||((b==4 || b==6 ||b==9 || b==11)&&(a>30))){
        cout<<"error";
    }
    else if(b==12){
        cout<<32-a;
    }
    else if(b==11){
        cout<<62-a;
    }
    else if(b==10){
        cout<<93-a;
    }
    else if(b==9){
        cout<<123-a;
    }
    else if(b==8){
        cout<<154-a;
    }
    else if(b==7){
        cout<<185-a;
    }
    else if(b==6){
        cout<<215-a;
    }
    else if(b==5){
        cout<<246-a;
    }
    else if(b==4){
        cout<<276-a;
    }
    else if(b==3){
        cout<<307-a;
    }
    else if(b==2){
        cout<<335-a;
    }
    else if(b==1){
        cout<<366-a;
    }
}
