#include <iostream>

using namespace std;

int main()
{
    int n,a0,a1,a2;
    cin>>n;
    a0=0;
    a1=1;
    a2=1;
    if(n==0){
        cout<<0;
    }
    else if(n==1){
        cout<<1;
    }
    else{
        for(int i=0;i<n-1;i++){
            a2=a0+a1;
            a0=a1;
            a1=a2;
    }
    cout<<a2;
    }
}
